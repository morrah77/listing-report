<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <h1><%= modelName %></h1>
    <table class='report'>
        <%- include ('headers/'+modelName) -%>
        <% models.forEach(function(model){ %>
            <%- include ('models/' + modelName, {model:model}) -%>
        <% }) %>
    </table>
  </body>
</html>
